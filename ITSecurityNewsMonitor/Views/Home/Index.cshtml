@{
    ViewData["Title"] = "Watchtower";
}

<div class="absolute z-10 w-full px-10 flex justify-center">
    <div class="pt-7 flex w-full max-w-6xl">
        <h1 class="text-3xl text-white font-bold">Watchtower</h1>

        <button class="ml-auto border px-3 py-2 rounded hover:bg-white hover:text-black text-white">
            Sign Up
        </button>
        <button class="ml-2 border px-3 py-2 rounded hover:bg-white hover:text-black text-white">
            Login
        </button>
    </div>
</div>

<div class="h-96 relative w-full">
    <!-- Background  -->
    <canvas id="background-canvas"
            width="32"
            height="12"
            class="absolute w-full h-full">
    </canvas>

    <!-- Headline  -->
    <div class="absolute top-1/2 mt-5 flex justify-center w-full px-10">
        <div class="w-full max-w-6xl flex">
            <div class="text-white">
                <span class="font-bold text-2xl">Fast, relevant and focused.</span><br />
                <span class="font-bold text-5xl">Better IT security news.</span>
            </div>
        </div>
    </div>
</div>

<!-- Call to Action  -->
<div class="flex justify-center px-10 mt-10">
    <div class="max-w-6xl w-full bg-purple-200 p-7 rounded-md flex">
        <span class="mr-2">IT-Security should be a community effort!</span>
        <button class="group flex">
            <span class="font-bold">Join the Beta!</span>
            <svg class="ml-2 mt-1 w-4 h-4 cursor-pointer group-hover:ml-4 transition-all"
                 aria-hidden="true"
                 focusable="false"
                 role="img"
                 xmlns="http://www.w3.org/2000/svg"
                 viewBox="0 0 448 512">
                <path d="M218.101 38.101L198.302 57.9c-4.686 4.686-4.686 12.284 0 16.971L353.432 230H12c-6.627 0-12 5.373-12 12v28c0 6.627 5.373 12 12 12h341.432l-155.13 155.13c-4.686 4.686-4.686 12.284 0 16.971l19.799 19.799c4.686 4.686 12.284 4.686 16.971 0l209.414-209.414c4.686-4.686 4.686-12.284 0-16.971L235.071 38.101c-4.686-4.687-12.284-4.687-16.97 0z"></path>
            </svg>
        </button>
    </div>
</div>

