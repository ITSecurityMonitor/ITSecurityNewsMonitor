@section Scripts {
    <script>
        if (typeof context == "undefined") {
            const c = document.getElementById("background-canvas");
            const context = c.getContext("2d");

            const col = function (x, y, r, g, b) {
                context.fillStyle = `rgba(${r}, ${g}, ${b}, 1)`;
                context.fillRect(x, y, 1, 1);
            };
            const R = function (x, y, t) {
                let value = Math.floor(
                    192 + 64 * Math.cos((x * x - y * y) / 300 + t)
                );
                return value;
            };

            const G = function (x, y, t) {
                let value = Math.floor(
                    192 +
                    64 *
                    Math.sin(
                        (x * x * Math.cos(t / 4) +
                            y * y * Math.sin(t / 3)) /
                        300
                    )
                );
                return value * 0.5;
            };

            const B = function (x, y, t) {
                let value = Math.floor(
                    192 +
                    64 *
                    Math.sin(
                        5 * Math.sin(t / 9) +
                        ((x - 100) * (x - 100) +
                            (y - 100) * (y - 100)) /
                        1100
                    )
                );
                return value * 1.2;
            };

            let time = 0;
            const run = function () {
                for (x = 0; x <= 35; x++) {
                    for (y = 0; y <= 35; y++) {
                        col(
                            x,
                            y,
                            R(x, y, time),
                            G(x, y, time),
                            B(x, y, time)
                        );
                    }
                }
                time = time + 0.01;
                window.requestAnimationFrame(run);
            };
            run();
        }
    </script>
}